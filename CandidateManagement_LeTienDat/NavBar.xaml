<UserControl x:Class="CandidateManagement_LeTienDat.NavBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CandidateManagement_LeTienDat"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top">
        <Button x:Name="CandidateProfile" Content="CandidateProfile" Height="33" Width="114" Background="#FF90EE90" Margin="5,0" Click="CandidateProfile_Click"/>
        <Button x:Name="JobPosting" Content="JobPosting" Height="33" Width="114" Background="#FFFFE4B5" Margin="5,0" Click="JobPosting_Click"/>
        <Button x:Name="Logout" Content="Logout" Height="33" Width="114" Background="#FFFFE4B5" Margin="5,0" Click="Logout_Click"/>
    </StackPanel>
</UserControl>
